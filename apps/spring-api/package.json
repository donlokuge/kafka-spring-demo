{
  "name": "spring-api",
  "version": "1.0.0",
  "private": true,
  "nx": {
    "targets": {
      "bootRun": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./mvnw spring-boot:run",
          "cwd": "apps/spring-api"
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./mvnw clean package",
          "cwd": "apps/spring-api"
        }
      }
    }
  }
}
